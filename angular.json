{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"web-components-cli": {
			"projectType": "application",
			"root": ".",
			"sourceRoot": "src",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"index": "webcomponents/index.html",
						"tsConfig": "tsconfig.wc.json",
						"polyfills": ["src/polyfills.ts"],
						"inlineStyleLanguage": "scss"
					},
					"configurations": {
						"oeb-version": {
							"main": "src/app/public/components/version.web-component.ts",
							"outputPath": "dist/webcomponents/oeb-version"
						},
						"oeb-skill-visualisation": {
							"main": "src/app/recipient/components/recipient-skill-visualisation/recipient-skill-visualisation.web-component.ts",
							"outputPath": "dist/webcomponents/oeb-skill-visualisation",
							"styles": [
								"src/assets/@concentricsky/badgr-style/dist/css/screen.css",
								"src/styles/oeb/oeb_styles.scss",
								"src/styles/google-webfonts/inter.scss",
								"src/styles/google-webfonts/nunito.scss",
								"src/styles/google-webfonts/rubik.scss",
								"node_modules/dialog-polyfill/dialog-polyfill.css",
								"src/breakdown/static/css/re-style.css",
								"node_modules/maplibre-gl/dist/maplibre-gl.css",
								"node_modules/ngx-markdown-editor/assets/highlight.js/agate.min.css",
								"src/app/recipient/components/recipient-skill-visualisation/recipient-skill-visualisation.component.scss"
							]
						}
					}
				}
			}
		},
		"badgr-ui-cli": {
			"root": ".",
			"sourceRoot": "src",
			"projectType": "application",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:application",
					"options": {
						"outputPath": {
							"base": "dist",
							"browser": ""
						},
						"index": "src/index.html",
						"tsConfig": "tsconfig.app.json",
						"polyfills": ["src/polyfills.ts"],
						"assets": [
							"src/assets",
							"src/breakdown",
							"src/_redirects",
							{
								"glob": "**/*",
								"input": "node_modules/ace-builds/src-min",
								"output": "./assets/ace-builds/"
							},
							{
								"glob": "**/*",
								"input": "./assets/@concentricsky/badgr-style/dist/images/",
								"output": "./assets/@concentricsky/badgr-style/dist/images/"
							}
						],
						"styles": [
							"src/assets/@concentricsky/badgr-style/dist/css/screen.css",
							"src/styles/oeb/oeb_styles.scss",
							"src/styles/google-webfonts/inter.scss",
							"src/styles/google-webfonts/nunito.scss",
							"src/styles/google-webfonts/rubik.scss",
							"node_modules/dialog-polyfill/dialog-polyfill.css",
							"src/breakdown/static/css/re-style.css",
							"node_modules/maplibre-gl/dist/maplibre-gl.css",
							"node_modules/ngx-markdown-editor/assets/highlight.js/agate.min.css"
						],
						"scripts": [
							"node_modules/ngx-markdown-editor/assets/highlight.js/highlight.min.js",
							"node_modules/ngx-markdown-editor/assets/marked.min.js",
							"node_modules/altcha/dist/altcha.js"
						],
						"extractLicenses": false,
						"sourceMap": true,
						"optimization": false,
						"namedChunks": true,
						"inlineStyleLanguage": "scss",
						"browser": "src/main.ts"
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"index": {
								"input": "src/index.production.html",
								"output": "index.html"
							},
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								}
							]
						},
						"staging": {
							"budgets": [
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"index": {
								"input": "src/index.staging.html",
								"output": "index.html"
							},
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.staging.ts"
								}
							]
						},
						"develop": {
							"budgets": [
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"index": {
								"input": "src/index.develop.html",
								"output": "index.html"
							},
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.develop.ts"
								}
							]
						}
					},
					"defaultConfiguration": ""
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"buildTarget": "badgr-ui-cli:build"
					},
					"configurations": {
						"production": {
							"buildTarget": "badgr-ui-cli:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"buildTarget": "badgr-ui-cli:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": ["zone.js", "zone.js/testing"],
						"tsConfig": "tsconfig.spec.json",
						"karmaConfig": "src/karma.conf.js",
						"styles": [
							"src/assets/@concentricsky/badgr-style/dist/css/screen.css",
							"node_modules/dialog-polyfill/dialog-polyfill.css"
						],
						"scripts": [
							"node_modules/ngx-markdown-editor/assets/marked.min.js",
							"node_modules/ace-builds/src-min/ace.js"
						],
						"assets": [
							"src/favicon.ico",
							"src/assets",
							{
								"glob": "**/*",
								"input": "node_modules/ace-builds/src-min",
								"output": "./assets/ace-builds/"
							},
							{
								"glob": "icons.svg",
								"input": "./assets/@concentricsky/badgr-style/dist/images",
								"output": "./assets/@concentricsky/badgr-style/dist/images"
							}
						],
						"inlineStyleLanguage": "scss"
					}
				}
			}
		}
	},
	"schematics": {
		"@schematics/angular:component": {
			"prefix": "app",
			"style": "css",
			"type": "component"
		},
		"@schematics/angular:directive": {
			"prefix": "app",
			"type": "directive"
		},
		"@schematics/angular:service": {
			"type": "service"
		},
		"@schematics/angular:guard": {
			"typeSeparator": "."
		},
		"@schematics/angular:interceptor": {
			"typeSeparator": "."
		},
		"@schematics/angular:module": {
			"typeSeparator": "."
		},
		"@schematics/angular:pipe": {
			"typeSeparator": "."
		},
		"@schematics/angular:resolver": {
			"typeSeparator": "."
		}
	},
	"cli": {
		"analytics": false
	}
}
